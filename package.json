{
    "name": "kivy-snippets",
    "publisher": "watchakorn-18k",
    "displayName": "Kivy-Snippets",
    "description": "Kivy Snippet and Syntax highlighting support VS Code ภาษาไทย",
    "version": "1.0.1",
    "icon":"images/icons_kv.png",
    "engines": {
        "vscode": "^1.58.0"
    },
    "keywords": [
        "kivy",
        "kvlang",
        "kv"
    ],
    "categories": [
        "Programming Languages",
        "Snippets"
    ],
      "repository": {
        "type": "git",
        "url": "https://github.com/watchakorn-18k/kivy-snippets"
      },
    "contributes": {
        "languages": [{
            "id": "kv",
            "aliases": ["Kivy", "kv"],
            "extensions": [".kv"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "kv",
            "scopeName": "source.kv",
            "path": "./syntaxes/kv.tmLanguage.json"
        }],
        "snippets": [
            {
                "language": "kv",
                "path": "./snippets/snippets.code-snippets"
            }
        ]
    }
}
